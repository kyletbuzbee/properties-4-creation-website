{
  "name": "properties4creations.com",
  "version": "1.0.0",
  "description": "Modern, accessible housing website for veterans and families in East Texas",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview --host",
    "clean": "rm -rf dist",
    "lint": "echo 'No linting configured yet'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:performance": "lighthouse http://localhost:3000 --output=json --output-path=./reports/lighthouse.json",
    "test:all": "npm run test && npm run test:e2e && npm run test:accessibility"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyletbuzbee/properties-4-creation-website.git"
  },
  "keywords": [
    "housing",
    "veterans",
    "affordable-housing",
    "section-8",
    "hud-vash",
    "east-texas"
  ],
  "author": "Properties 4 Creations",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kyletbuzbee/properties-4-creation-website/issues"
  },
  "homepage": "https://properties4creations.com",
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "autoprefixer": "^10.4.23",
    "axe-core": "^4.11.0",
    "babel-jest": "^30.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lighthouse": "^13.0.1",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "vite": "^7.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
